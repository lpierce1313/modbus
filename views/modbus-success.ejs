<!DOCTYPE html>
  <html>
     <head>
       <% include partials/head %>
     </head>
     <header>
       <% include partials/header %>
     </header>
     <main>
       <br /> <br> <br> <br>
       <div class="container">
         <div class="jumbotron">
          <div class="row">
            <div class="col s12 m12">
              <div class="card z-depth-4">
                <div class="card-content">
                    <br>
                  <span class="card-title center-align"><h4 style="color:#388e3c;">Success, you have entered: </h4></span> <br>
                    <div class="container">
                      <div class="row">
                        <div class="col s6">
                          <% if(data.comPort) {  %>
                              <p> <b> Com Port: </b> <%= data.comPort %> </p>
                          <% } %>
                        </div>
                        <div class="col s6">
                          <% if(data.unitAdd) {  %>
                              <p> <b> Unit Address: </b> <%= data.unitAdd %> </p>
                          <% } %>
                        </div>
                        <div class="col s6">
                          <% if(data.startReg) {  %>
                              <p> <b> Start Register: </b> <%= data.startReg %> </p>
                          <% } %>
                        </div>
                        <div class="col s6">
                          <% if(data.numReg) {  %>
                              <p> <b> Number of Registers: </b> <%= data.numReg %> </p>
                          <% } %>
                        </div>
                        <div class="col s6">
                          <% if(data.dataType) {  %>
                              <p> <b> Data Type: </b> <%= data.dataType %> </p>
                          <% } %>
                        </div>
                        <div class="col s6">
                          <% if(data.parity == 0) {  %>
                              <p> <b> Parity: </b> None (0) </p>
                          <% } else if(data.parity == 1) {%>
                              <p> <b> Parity: </b> Odd (1) </p>
                          <% } else { %>
                              <p> <b> Parity: </b> Even (2) </p>
                          <% } %>
                        </div>
                        <div class="col s6">
                          <% if(data.code < 9) {  %>
                              <p> <b> Function Code: </b> FC0<%= data.code %> </p>
                          <% } else { %>
                              <p> <b> Function Code: </b> FC<%= data.code %> </p>
                          <% } %>
                        </div>
                        <div class="col s6">
                          <% if(data.speed) {  %>
                              <p> <b> Baud Rate (speed): </b> <%= data.speed %> </p>
                          <% } %>
                        </div>
                        <div class="col s6">
                          <% if(data.sign < "u") {  %>
                              <p> <b> Sign: </b> Unsigned </p>
                          <% } else { %>
                              <p> <b> Sign: </b> Signed </p>
                          <% } %>
                        </div>
                        <div class="col s6">
                          <% if(data.bit == 16) {  %>
                              <p> <b> Number of Bits: </b> 16 bits </p>
                          <% } else { %>
                              <p> <b> Number of Bits: </b> 32 bits </p>
                          <% } %>
                        </div>
                      </div> <br>
                      <table class="striped">
                      <thead >
                        <tr style="font-size:18px;">
                            <th>Register Number </th>
                            <th>Value</th>
                        </tr>
                      </thead>
                      <tbody style="font-size:16px;">
                        <% var i = 1; %>
                        <% data.result.forEach(function(val) { %>
                          <tr>
                            <td> Register <%= i %> </td>
                            <td> <%= val %> </td>
                          </tr>
                          <% i++ %>
                        <% }); %>
                      </tbody>
                    </table>
                    </div>
                </div>
              <br> <br>
              </div>
            </div>
          </div>
         </div>
       </div>
     </main>
       <br> <br> <br> <br>
     <footer>
       <% include partials/footer %>
     </footer>
  </html>
